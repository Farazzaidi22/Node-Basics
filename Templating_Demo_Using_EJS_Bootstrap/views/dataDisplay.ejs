<%- include('partials/heads.ejs') %> 

<body>
  <%- include('partials/navbar.ejs') %> 
   
    <h1>Browsing the <%= name %></h1>
    <h2><%= description %></h2>
    <p><%= subscribers %></p>

    <% for(let post of posts) { %>
    <article>
      <p><%= post.title %> - <b> <%= post.author %> </b></p>

      <% if(post.img) { %>
      <img src="<%= post.img  %> " alt="" />
      <% } %>
    </article>
    <% } %>
  </body>
</html>
